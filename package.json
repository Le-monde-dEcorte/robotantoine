{
	"name": "sleepymaid-ts-interaction",
	"version": "1.0.0",
	"repository": "https://github.com/Le-monde-dEcorte/sleepymaid-ts.git",
	"author": "Ecorte",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"services/*",
		"packages/*"
	],
	"volta": {
		"node": "16.14.2",
		"yarn": "1.22.18"
	},
	"packageManager": "yarn@1.22.18",
	"devDependencies": {
		"@types/prettier": "2.4.4",
		"@typescript-eslint/eslint-plugin": "5.18.0",
		"@typescript-eslint/parser": "5.18.0",
		"eslint": "8.12.0",
		"eslint-config-prettier": "8.5.0",
		"prettier": "2.6.2",
		"prisma": "3.11.1",
		"turbo": "1.1.10",
		"typescript": "4.6.3",
		"husky": "7.0.4"
	},
	"scripts": {
		"build": "yarn turbo run build",
		"start": "yarn turbo run start --scope=@sleepymaid-ts/bot",
		"starttest": "yarn turbo run start --scope=@sleepymaid-ts/testbot",
		"lint": "yarn eslint --fix --ext ts,tsx packages",
		"testlint": "yarn eslint --ext ts,tsx packages",
		"format": "yarn prettier --write .",
		"prepare": "husky install"
	}
}
