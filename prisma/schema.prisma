datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:admin@localhost:5432/mondecorte?schema=mondecorte"
}

generator client {
  provider = "prisma-client-js"
}

model mondecorte {
  user_id        String  @unique
  custom_role_id String?
  points         Int?    @default(0)
  social_credit  Int?    @default(500)
  vote           String?
}
