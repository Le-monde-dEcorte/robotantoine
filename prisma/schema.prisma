datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model mondecorte {
  user_id        String  @unique
  custom_role_id String?
  points         Int?    @default(0)
  social_credit  Int?    @default(500)
  vote           String?
}
