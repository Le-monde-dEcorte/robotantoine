generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://localhost:27017/sleepymaid"
}

model mondecorte {
  id_          String  @id @default(dbgenerated()) @map("_id") @db.ObjectId
  v            Int?    @map("__v")
  crole        String?
  id           String
  points       Int
  socialcredit Int?
  vote         String?

  @@index([id], map: "id_1")
  @@index([crole], map: "crole_1")
}
