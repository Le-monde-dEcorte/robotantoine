generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://root:prisma@localhost:27017/sleepymaid?authSource=admin&retryWrites=true&w=majority"
}

model mondecorte {
  id_          String  @id @default(dbgenerated()) @map("_id") @db.ObjectId
  crole        String?
  id           String  @unique
  points       Int?
  socialcredit Int?
  vote         String?
}
